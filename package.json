{
  "name": "markbase-template-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run unzip && npx concurrently \"next dev\" \"npm run build:css:watch\"",
    "build": "npm run unzip && npm run obsidian:generate && npm run build:css && next build",
    "postbuild": "next-sitemap --config next-sitemap-config.js",
    "start": "npm run dev",
    "lint": "next lint",
    "build:css": "npx tailwindcss -i ./assets/styles/input.css -o ./assets/styles/output.css",
    "build:css:watch": "npx tailwindcss -i ./assets/styles/input.css -o ./assets/styles/output.css --watch",
    "obsidian:generate": "chmod +x ./utils/obsidian-export.bin && ./utils/obsidian-export.bin ./content ./content",
    "obsidian:generate:local": "./utils/obsidian-export.bin ./content ./content",
    "unzip": "unzip -o ./content/content-bundle.zip -d ./content"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.0",
    "@heroicons/react": "^1.0.6",
    "@types/next-seo": "^2.1.2",
    "@types/react-cytoscapejs": "^1.2.2",
    "cytoscape": "^3.21.1",
    "directory-tree": "^3.2.2",
    "gray-matter": "^4.0.3",
    "lunr": "^2.3.9",
    "markdown-link-extractor": "^4.0.1",
    "marked": "^4.0.15",
    "next": "12.1.6",
    "next-seo": "^5.4.0",
    "next-sitemap": "^2.5.25",
    "react": "18.1.0",
    "react-cytoscapejs": "^1.2.1",
    "react-dom": "18.1.0",
    "recursive-readdir": "^2.2.2",
    "remove-markdown": "^0.5.0",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/lunr": "^2.3.4",
    "@types/marked": "^4.0.3",
    "@types/node": "17.0.31",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.3",
    "@types/recursive-readdir": "^2.2.1",
    "autoprefixer": "10.4.5",
    "concurrently": "^7.1.0",
    "eslint": "8.14.0",
    "eslint-config-next": "12.1.6",
    "postcss": "^8.4.13",
    "tailwind-scrollbar": "^1.3.1",
    "tailwindcss": "^3.0.24",
    "typescript": "4.6.4"
  }
}
